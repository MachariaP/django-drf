# Django Configuration
SECRET_KEY=your-secret-key-here
DEBUG=True

# Database Configuration
# PostgreSQL example: postgresql://user:password@localhost:5432/dbname
# MySQL example: mysql://user:password@localhost:3306/dbname
# Leave empty to use SQLite (default)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Cache Configuration
# Redis example: redis://localhost:6379/0
# Leave empty to use local memory cache (default)
REDIS_URL=redis://localhost:6379/0

# AWS Configuration (Optional)
# Used for S3 storage, SES email, etc.
AWS_ACCESS_KEY_ID=your-aws-key
AWS_SECRET_ACCESS_KEY=your-aws-secret
AWS_STORAGE_BUCKET_NAME=your-bucket-name
AWS_S3_REGION_NAME=us-east-1
